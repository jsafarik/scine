{
	"name": "scine",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"register-channel": "{ test -n \"$PROJECT_ID\" || export $(grep -v '^#' .env.local | xargs); } && node --experimental-loader ts-node/esm --experimental-specifier-resolution=node ./scripts/registerChannel.ts",
		"update-videos": "{ test -n \"$PROJECT_ID\" || export $(grep -v '^#' .env.local | xargs); } && node --experimental-loader ts-node/esm --experimental-specifier-resolution=node ./scripts/updateVideos.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.25.1",
		"@types/xml2js": "^0.4.14",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^9.6.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.38.0",
		"firebase": "^10.12.2",
		"firebase-tools": "^13.12.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"sass": "^1.77.4",
		"svelte": "^4.2.17",
		"svelte-check": "^3.6.6",
		"ts-node": "^10.9.2",
		"tslib": "^2.4.1",
		"typescript": "^5.4.5",
		"vite": "^4.4.2",
		"xml2js": "^0.6.2"
	},
	"type": "module"
}
